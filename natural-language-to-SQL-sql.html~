<pre class="src src-python">prices<span style="color: #2aa198;">[</span>1,2,3,4,5<span style="color: #2aa198;">]</span>
<span style="color: #268bd2;">N</span>=<span style="color: #839496; font-weight: bold;">len</span><span style="color: #2aa198;">(</span>prices<span style="color: #2aa198;">)</span>
<span style="color: #268bd2;">max_ij</span>=<span style="color: #2aa198;">(</span>0,0<span style="color: #2aa198;">)</span>
<span style="color: #268bd2;">max_num</span>=prices<span style="color: #2aa198;">[</span>max_ij<span style="color: #b58900;">[</span>1<span style="color: #b58900;">]</span><span style="color: #2aa198;">]</span>-prices<span style="color: #2aa198;">[</span>max_ij<span style="color: #b58900;">[</span>0<span style="color: #b58900;">]</span><span style="color: #2aa198;">]</span>
<span style="color: #859900; font-weight: bold;">for</span> i <span style="color: #859900; font-weight: bold;">in</span> <span style="color: #839496; font-weight: bold;">range</span><span style="color: #2aa198;">(</span>N<span style="color: #2aa198;">)</span>:
    <span style="color: #859900; font-weight: bold;">for</span> j <span style="color: #859900; font-weight: bold;">in</span> <span style="color: #839496; font-weight: bold;">range</span><span style="color: #2aa198;">(</span>N<span style="color: #2aa198;">)</span>:
        <span style="color: #859900; font-weight: bold;">if</span> j&gt;i:
            <span style="color: #268bd2;">new_max_num</span>=prices<span style="color: #2aa198;">[</span>j<span style="color: #2aa198;">]</span>-prices<span style="color: #2aa198;">[</span>i<span style="color: #2aa198;">]</span>
            <span style="color: #859900; font-weight: bold;">if</span> new_max_num&gt;max_num:
                <span style="color: #268bd2;">max_num</span>=new_max_num
                <span style="color: #268bd2;">max_ij</span>=<span style="color: #2aa198;">(</span>i,j<span style="color: #2aa198;">)</span>
